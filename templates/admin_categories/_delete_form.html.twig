<form style="margin-left:2em" method="post" action="{{ path('app_admin_categories_delete', {'id': category.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.id) }}">
    <button class="btn btn-danger" style="margin-top:80px">Supprimer</button>
</form>
